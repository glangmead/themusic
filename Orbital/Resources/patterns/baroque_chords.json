    "tracks": [
      {
        "name": "Baroque Chords",
        "presetFilename": "prophet_brass",
        "numVoices": 20,
        "presetModulatorNames": ["ampMod", "detuneMod"],
        "noteMaterial": "chords",
        "sustainEmitter": "sustain",
        "gapEmitter": "gap"
