        "arg2": 0.6
      },
      {
        "name": "detuneVal",
        "outputType": "float",
        "function": "randFloat",
        "arg1": -5,
        "arg2": 5
      },
      {
        "name": "sustain",
        "outputType": "float",
        "function": "randFloat",
        "arg1": 3,
        "arg2": 8
      },
      {
        "name": "gap",
        "outputType": "float",
        "function": "randFloat",
        "arg1": 4,
        "arg2": 10
      }
    ],
    "noteMaterials": [
      {
        "type": "scaleMaterial",
        "name": "chords",
        "root": "C",
        "scale": "major",
        "intervals": [
          [0, 2, 4],
          [5, 0, 2],
          [3, 5, 0],
          [1, 3, 5],
          [4, 6, 1],
          [2, 4, 6],
          [2, 4, 0],
          [5, 0, 3],
          [3, 5, 1],
          [6, 1, 4],
          [4, 6, 2],
          [0, 2, 5],
          [1, 3, 6],
          [4, 0, 2]
