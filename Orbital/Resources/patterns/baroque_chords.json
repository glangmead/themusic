        "intervalPickerEmitter": "chordPicker",
        "octaveEmitter": "octaves"
      }
    ],
    "presetModulators": [
      {
        "name": "ampMod",
        "targetHandle": "overallAmp",
        "floatEmitter": "overallAmpVal"
      },
      {
        "name": "detuneMod",
        "targetHandle": "overallCentDetune",
        "floatEmitter": "detuneVal"
