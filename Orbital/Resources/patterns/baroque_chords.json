{
  "name": "Baroque Chords",
  "tableTracks": {
    "name": "Baroque Chords",
    "hierarchy": {
      "root": "C",
      "scale": "major",
      "chord": {"degrees": [0, 2, 4], "inversion": 0}
    },
    "emitters": [
      {
        "name": "chordTimer",
        "outputType": "float",
        "function": "randFloat",
        "arg1": 3,
        "arg2": 8
      },
      {
        "name": "octaves",
        "outputType": "octave",
        "function": "random",
        "candidates": ["1", "1", "2", "2", "2", "3", "3", "4"]
      },
      {
        "name": "overallAmpVal",
        "outputType": "float",
        "function": "exponentialRandFloat",
        "arg1": 0.3,
        "arg2": 0.6
      },
      {
        "name": "detuneVal",
        "outputType": "float",
        "function": "randFloat",
        "arg1": -5,
        "arg2": 5
      },
      {
        "name": "sustain",
        "outputType": "float",
        "function": "randFloat",
        "arg1": 3,
        "arg2": 8
      },
      {
        "name": "gap",
        "outputType": "float",
        "function": "randFloat",
        "arg1": 4,
        "arg2": 10
      }
    ],
    "hierarchyModulators": [
      {
        "name": "chordShifter",
        "level": "chord",
        "operation": "markovChord",
        "n": 1,
        "fireIntervalEmitter": "chordTimer"
      }
    ],
    "noteMaterials": [
      {
        "type": "hierarchyChord",
        "name": "chords",
        "voicing": "closed",
        "octaveEmitter": "octaves"
      }
    ],
    "presetModulators": [
      {
        "name": "ampMod",
        "targetHandle": "overallAmp",
        "floatEmitter": "overallAmpVal"
      },
      {
        "name": "detuneMod",
        "targetHandle": "overallCentDetune",
        "floatEmitter": "detuneVal"
      }
    ],
    "tracks": [
      {
        "name": "Baroque Chords",
        "presetFilename": "warm_analog_pad",
        "numVoices": 20,
        "presetModulatorNames": ["ampMod", "detuneMod"],
        "noteMaterial": "chords",
        "sustainEmitter": "sustain",
        "gapEmitter": "gap"
      }
    ]
  }
}
