{
 "name"   : "Aurora Borealis",
 "rose"   : {"freq": 0.25, "leafFactor": 2, "phase": 3.14, "amp": 4},
 "effects": {"reverbPreset": 1, "delayTime": 0, "delayLowPassCutoff": 100000, "delayFeedback": 0, "reverbWetDryMix": 100, "delayWetDryMix": 0},
 "arrow"  : {
  "compose": { "arrows": [
    {
     "prod": { "of": [
       {
        "crossfadeEqPow": { "name": "oscCrossfade", 
          "mixPoint": { "compose": {"arrows": [{"identity": {}}, {"noiseSmoothStep": {"noiseFreq": 2, "min": 0, "max": 2}}]}}, 
          "of": [
          {
           "prod": { "of": [
             { "const": {"val": 1.0, "name": "osc1Mix"} },
             { 
              "compose": { "arrows": [
                {
                 "sum": { "of": [
                   { "prod": { "of": [ 
                    { "const": {"name": "freq", "val": 300} }, 
                    { "constOctave": {"name": "osc1Octave", "val": 0} },
                    { "constCent": {"name": "osc1CentDetune", "val": 0} },
                    { "identity": {}}  
                   ]}},
                   { "prod": { "of": [
                      { "line": {"duration": 7, "min": 0, "max": 1} },
                      { "compose": { "arrows": [
                         { "prod": { "of": [
                           { "const": {"val": 1, "name": "vibratoFreq"} },
                           { "identity": {} }
                         ]}},
                         { "osc": {"name": "vibratoOsc", "shape": "sineOsc", "width": { "const": {"name": "osc1VibWidth", "val": 1 } } } }
                      ]}}
                    ]}
                   }
                 ]}
                },
                { "osc": {"name": "osc1", "shape": "squareOsc", "width": { "noiseSmoothStep": {"noiseFreq": 1, "min": 0.2, "max": 0.8} }} },
                { "choruser": { "name": "osc1Choruser", "valueToChorus": "freq", "chorusCentRadius": 0, "chorusNumVoices": 1 } }
              ]}}
           ]}
          },
          {
           "prod": { "of": [
             { "const": {"val": 1.0, "name": "osc2Mix"} },
             {
              "compose": { "arrows": [
                {
                 "sum": { "of": [
                   { 
                    "prod": { "of": [ 
                     { "const": {"name": "freq", "val": 300} }, 
                     { "constOctave": {"name": "osc2Octave", "val": 0} },
                     { "constCent": {"name": "osc2CentDetune", "val": 0} },
                     {"identity": {}}
                    ]}
                   },
                   { "prod": { "of": [
                      { "line": {"duration": 7, "min": 0, "max": 1} },
                       { "compose": { "arrows": [
                          { "prod": { "of": [
                            { "const": {"val": 1, "name": "vibratoFreq"} },
                            { "identity": {} }
                          ]}},
                          { "osc": {"name": "vibratoOsc", "shape": "sineOsc", "width": { "const": {"name": "osc2VibWidth", "val": 1} }} }
                       ]}}
                     ]}
                    }
                 ]}
                },
                { "osc": {"name": "osc2", "shape": "sawtoothOsc", "width": { "const": {"name": "osc2Width", "val": 1} }} },
                { "choruser": { "name": "osc2Choruser", "valueToChorus": "freq", "chorusCentRadius": 0, "chorusNumVoices": 1 } }
              ]}
             }
           ]}
          },
          {
           "prod": { "of": [
             { "const": {"val": 1.0, "name": "osc3Mix"} },
             {
              "compose": { "arrows": [
                {
                 "sum": { "of": [
                   { "prod": { "of": [ 
                     { "const": {"name": "freq", "val": 300} }, 
                     { "constOctave": {"name": "osc3Octave", "val": 0} },
                     { "constCent": {"name": "osc3CentDetune", "val": 0} },
                     {"identity": {}} 
                   ]}},
                   { "prod": { "of": [
                      { "line": {"duration": 7, "min": 0, "max": 1} },
                       { "compose": { "arrows": [
                          { "prod": { "of": [
                            { "const": {"val": 1, "name": "vibratoFreq"} },
                            { "identity": {} }
                          ]}},
                          { "osc": {"name": "vibratoOsc", "shape": "sineOsc", "width": { "const": {"name": "osc3VibWidth", "val": 1} }} }
                       ]}}
                     ]}
                    }

                 ]}
                },
                { "osc": {"name": "osc3", "shape": "noiseOsc", "width": { "const": {"name": "osc3Width", "val": 1} }} },
                { "choruser": { "name": "osc3Choruser", "valueToChorus": "freq", "chorusCentRadius": 0, "chorusNumVoices": 1 } }
               ]
              }
             }
           ]}
          }
        ]}
       },
       { "envelope": { "decay": 2.5, "sustain": 0, "attack": 2.5, "name": "ampEnv", "release": 0.1, "scale": 1} }
      ]}
    },
    {
     "lowPassFilter": {
       "cutoff"   : 
        {"sum": { "of": [
          { "const": {"name": "cutoffLow", "val": 50} },
          { "prod": { "of": [
            { "const": {"name": "cutoff", "val": 5000} },
            { "envelope": { "release": 0.1, "scale": 1, "name": "filterEnv", "attack": 0.1, "decay": 0.1, "sustain": 1 } }
          ]}}
       ]}},
       "resonance": { "const": {"name": "resonance", "val": 1.1} },
       "name"     : "filter"
     }
   }]
  }
 }
}
