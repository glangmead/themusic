{
  "name": "Aurora Arpeggio",
  "presetName": "auroraBorealis",
  "numVoices": 20,
  "noteGenerator": {
    "melodic": {
      "scales": {
        "candidates": ["lydian"],
        "emission": "cyclic"
      },
      "roots": {
        "candidates": ["C", "E", "G"],
        "emission": {
          "waiting": {
            "iterator": "cyclic",
            "timeBetweenChanges": { "rand": { "min": 10, "max": 25 } }
          }
        }
      },
      "octaves": {
        "candidates": [2, 2, 2, 2, 2, 2, 3, 3, 3, 3, 3, 3, 3, 3, 4, 4, 4, 5],
        "emission": "random"
      },
      "degrees": {
        "candidates": [0, 1, 2, 3, 4, 5, 6],
        "emission": "shuffled"
      }
    }
  },
  "sustain": { "random": { "min": 5.0, "max": 10.0 } },
  "gap":     { "random": { "min": 0.1, "max": 0.5 } },
  "modulators": [
    {
      "target": "overallAmp",
      "arrow": { "exponentialRand": { "min": 0.3, "max": 0.6 } }
    },
    {
      "target": "vibratoAmp",
      "arrow": { "exponentialRand": { "min": 0.00002, "max": 0.0001 } }
    },
    {
      "target": "vibratoFreq",
      "arrow": { "rand": { "min": 1.0, "max": 25.0 } }
    }
  ]
}
