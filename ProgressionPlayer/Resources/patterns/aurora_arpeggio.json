{
  "name": "Aurora Arpeggio",
  "presetName": "auroraBorealis",
  "numVoices": 20,
  "noteGenerator": {
    "melodic": {
      "scale": "lydian",
      "root": "C",
      "octaves": [2, 2, 2, 2, 2, 2, 3, 3, 3, 3, 3, 3, 3, 3, 4, 4, 4, 5],
      "degrees": [0, 1, 2, 3, 4, 5, 6],
      "ordering": "shuffled",
      "rootProgression": {
        "roots": ["C", "E", "G"],
        "waitMin": 10,
        "waitMax": 25
      }
    }
  },
  "sustain": { "random": { "min": 5.0, "max": 10.0 } },
  "gap":     { "random": { "min": 0.1, "max": 0.5 } },
  "modulators": [
    {
      "target": "overallAmp",
      "arrow": { "exponentialRand": { "min": 0.3, "max": 0.6 } }
    },
    {
      "target": "vibratoAmp",
      "arrow": { "exponentialRand": { "min": 0.00002, "max": 0.0001 } }
    },
    {
      "target": "vibratoFreq",
      "arrow": { "rand": { "min": 1.0, "max": 25.0 } }
    }
  ]
}
